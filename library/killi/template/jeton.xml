<?xml version="1.0" encoding="utf-8" ?>
<data xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../library/killi/template/validator.xsd">

	<header/>
	<menu/>
	<title string="Jeton"/>
	<navigator object="jeton" create="0"/>

	<list object="jeton" key="jeton_id">
		<field object="jeton" attribute="code" search="1"/>
		<field object="jeton" attribute="destinataire_id" search="1"/>
		<field object="jeton" attribute="actif" search="1"/>
		<field object="jeton" attribute="type_jeton_id" search="1"/>
		<field object="jeton" attribute="object_id" search="1"/>
		<field object="jeton" attribute="end_time" search="1"/>
	</list>

	<create object="jeton">
		<notebook>
			<page string="Informations">
				<field object="jeton" attribute="code"/>
				<field object="jeton" attribute="destinataire_id"/>
				<field object="jeton" attribute="actif"/>
				<field object="jeton" attribute="type_jeton_id"/>
				<field object="jeton" attribute="object_id"/>
				<field object="jeton" attribute="end_time"/>
			</page>
		</notebook>
	</create>

	<form object="jeton">
		<notebook>
			<page string="Informations">
				<field object="jeton" attribute="code"/>
				<field object="jeton" attribute="destinataire_id"/>
				<field object="jeton" attribute="actif"/>
				<field object="jeton" attribute="type_jeton_id"/>
				<field object="jeton" attribute="object_id"/>
				<field object="jeton" attribute="end_time"/>
			</page>
		</notebook>
	</form>
	
	<panel>
		<notebook>
			<page string="Jeton">
				<jeton/>
				<newline />
				<listing object="jeton" data_src="jeton.getJetonList" string="Liste des jetons" lazy_loading="0">
					<field object="jeton" attribute="code" readonly="1"/>
					<field object="jeton" attribute="type_jeton_id" readonly="1"/>
					<field object="jeton" attribute="destinataire_id" readonly="1"/>
					<fieldaction object="jeton" method="goToURL" string="Aller vers" confirm="1"/>
					<fieldaction object="jeton" method="sendURL" string="Envoyer le lien"/>
					<fieldaction object="jeton" method="changeJetonState" string="Changer Statut"/>
					<field object="jeton" attribute="actif" readonly="1"/>
				</listing>
			</page>
		</notebook>
	</panel>

</data>