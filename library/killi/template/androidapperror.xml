<?xml version="1.0" encoding="utf-8" ?>
<data xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="validator.xsd">

   <header/>
   <menu/>
   <title string="Android Throwable Error"/>
   <navigator object="androidapperror"/>

   <list object="androidapperror" key="android_app_error_id">
