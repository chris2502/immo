	  <field object="androidapperror" attribute="err_backtrace" />
	  <field object="androidapperror" attribute="alert_is_sended" />
   </create>
   
   <form object="androidapperror">
       <notebook>
	      <page string="Erreur Android">
			  <field object="androidapperror" attribute="date_err_client" format="Y-m-d H:i:s" readonly="1" />
			  <field object="androidapperror" attribute="date_err_server" format="Y-m-d H:i:s" readonly="1" />
