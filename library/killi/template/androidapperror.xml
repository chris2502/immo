	  <button string="Supprimer la sélection !" action="androidapperror.unlinkSelected" width="250px"/>
	  <field object="androidapperror" attribute="date_err_client" format="Y-m-d H:i:s"  search="1" />
	  <field object="androidapperror" attribute="date_err_server" format="Y-m-d H:i:s" search="1" />
	  <field object="androidapperror" attribute="app_name" search="1" />
	  <field object="androidapperror" attribute="device_data" search="1" />
	  <field object="androidapperror" attribute="err_type" search="1" />
