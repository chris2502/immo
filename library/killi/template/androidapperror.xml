			  <field object="androidapperror" attribute="device_data" readonly="1" />
			  <field object="androidapperror" attribute="err_type" readonly="1" />
			  <field object="androidapperror" attribute="err_cause" readonly="1" />
			  <field object="androidapperror" attribute="err_msg" readonly="1" />
			  <field object="androidapperror" attribute="err_backtrace" readonly="1" />
			  <field object="androidapperror" attribute="alert_is_sended" readonly="1" />
	      </page>
